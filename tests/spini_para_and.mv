(* 
   a0b0 a0b2 a2b0 
   a1b1 a1b1 a0b1
   a2b2 a2b0 a1b2
*)

proc AND:
  inputs: a[0:2], b[0:2]
  outputs: c[0:2]
  shares: ab[0:2]
  randoms: r[0:2];

  ab := a * b;    
  c   = ![ab + r]; 
  ab := a * (b>>1);
  c   = ![c + ab];
  ab := (a>>1) * b;
  c   = ![c + ab];
  c   = ![c + (r>>1)]; (* This can be c := c + (r>>1) for NI *)
end
verbose 1
noglitch SPINI AND

  
